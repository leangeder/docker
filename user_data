    -
        path: /etc/environment
        permissions: '0644'
        owner: root
        content: "COREOS_PUBLIC_IPV4=195.154.103.115\nCOREOS_PRIVATE_IPV4=127.0.0.1\nDOCKER_OPTS=--insecure-registry='195.154.103.115'\n"
coreos:
    fleet:
        public-ip: "$public_ipv4"
        metadata="name=master,provider=online,region=guadeloupe"
    update:
        group: stable
        reboot-strategy: reboot
        server: https://customer.update.core-os.net/v1/update/
    etcd:
        name: "$HOSTNAME"
        discovery: https://discovery.etcd.io/07c685b7e13e3ccc6eeb015dfab7bd2b
        # multi-region and multi-cloud deployments need to use $public_ipv4
        addr: $public_ipv4:4001
        peer-addr: $public_ipv4:7001
    units:
        - name: 00-eno1.network
          runtime: true
          content: "[Match]\nName=eno1\n\n[Network]\nDHCP=yes\n\n[DHCP]\nUseMTU=9000\n"
        - name: etcd.service
          command: start
        - name: fleet.service
          command: start
